{
    "plugins": [],
    "opts": {
        "template": "node_modules/docdash"
    },
    "recurseDepth": 10,
    "source": {
        "include": [
            "./src/resources/users",
            "./src/resources/tasks",
            "./src/resources/boards",
            "./src/resources/columns"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}